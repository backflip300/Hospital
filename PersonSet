package hospital;

public abstract class PersonSet {
	protected  int x;
	protected int y;
	protected static int radius;
	protected static int health;
	protected static int xp;
	protected static double speed;
	
	

	protected PersonSet(int x, int y) {
		this.x = x;
		this.y = y;
		radius = 10;
		
	}

	public  int getX() {
		return x;
	}

	public  int getY() {
		return y;
	}

	public static int getRadius() {
		return radius;
	}

	public static double getSpeed() {
		return speed;
	}

	public static int getHealth() {
		return health;
	}
}
